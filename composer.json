{
    "name": "theideabureau/laravel-nova-enum-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova",
        "enum"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1.0",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/http": "^9.0",
        "laravel/nova": "^3.0 | ^4.0"
    },
    "autoload": {
        "psr-4": {
            "TheIdeaBureau\\EnumField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": []
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
